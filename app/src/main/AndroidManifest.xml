<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cn.finalteam.okhttpfinal.sample" >

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".IApplication">
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewGameListActivity"/>
        <activity android:name=".DownloadManangerActivity"/>
        <activity android:name=".UploadActivity" />
        <activity android:name=".DownloadActivity"/>
        <activity android:name=".HttpRequestCallbackJsonActivity"/>
        <activity android:name=".HttpRequestCallbackStringActivity" />
        <activity android:name=".OtherFuncationActivity"/>

    </application>

</manifest>
